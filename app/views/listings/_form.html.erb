<%= bootstrap_form_for @listing do |f| %>
  <%= f.text_field :title %>
  <%= f.text_field :pet_name %>
  <%= f.number_field :price %>
  <%= f.file_field :avatars, multiple: true %>

  <%= f.submit "Submit", data: {disable_with: 'Saving...'}, class: "btn btn-block btn-default", id: "submit-listing"%>
<% end %>
